            /*Nil -> Nil*/
            /*Cons(a,as) -> Cons(fn(a), map(as, fn))*/

fun map(list, fn) -> 
    case list of
        Nil -> Nil
        Cons(a,as) -> Cons(fn(a), map(as, fn))


fun factorial(x) -> 
    case x of
        1 -> 1
        _ -> factorial((x - 1)) * x

fun id(x) -> 
    x

Given Num(a). fun square(x : a) : a -> 
    x * x

